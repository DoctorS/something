@import "reset.styl"
html, body
  width: 100%
  height: 100%

body
  padding: 20px
  font 14px / 18px Tahoma, Arial, sans-serif
  color #222

h1
  margin-bottom: 15px

p
  margin-bottom: 10px

.entities
  margin-bottom: 20px
  border-right 1px solid #dedede
  border-bottom 1px solid #dedede

  .entity
    display flex

    > div
      width: 4.7619%
      text-align center
      line-height: 24px
      border-top 1px solid #dedede
      border-left 1px solid #dedede
